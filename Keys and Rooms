class Solution:
    def canVisitAllRooms(self, rooms: List[List[int]]) -> bool:
        n = len(rooms)
        opened = [False]*n
        opened[0] = True
        queue = [0]
        while(len(queue)!=0):
            curr = queue[0]
            for key in rooms[curr]:
                if(opened[key]==False):
                    opened[key]=True
                    queue.append(key)
            queue.pop(0)
        for each in opened:
            if(each==False):
                return False
        return True
